<script type="text/javascript">
	$sns_jq(document).ready(function($) {
		$('#sns_sidenav li.level0.parent > a').addClass ('subhead');
		$('#sns_sidenav li.level0.parent > a').after ('<a href="#" title="" class="toggle"></a>');
